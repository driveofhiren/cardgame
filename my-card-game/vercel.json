{
	"version": 2,
	"builds": [
		{
			"src": "deck.js",
			"use": "@vercel/node"
		},
		{
			"src": "public/**/*",
			"use": "@vercel/static"
		}
	],
	"routes": [
		{
			"src": "/manifest.json",
			"dest": "/public/manifest.json"
		},
		{
			"src": "/favicon.ico",
			"dest": "/public/favicon.ico"
		},
		{
			"src": "/logo192.png",
			"dest": "/public/logo192.png"
		},
		{
			"src": "/logo512.png",
			"dest": "/public/logo512.png"
		},
		{
			"src": "/(.*)",
			"dest": "/public/index.html"
		}
	]
}
